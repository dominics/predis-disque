{
  "name": "varspool/predisque",
  "description": "Disque client based on Predis",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Dominic Scheirlinck",
      "email": "dominic@varspool.com"
    }
